<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef Name="VFEPD_ColourableVehicleBase" ParentName="BuildingBase" Abstract="True">
		<building>
			<paintable>true</paintable>
		</building>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Explosive">
				<wickTicks>360</wickTicks>
				<explosiveRadius>7.9</explosiveRadius>
				<explosiveDamageType>Bomb</explosiveDamageType>
				<explodeOnKilled>true</explodeOnKilled>
				<startWickHitPointsPercent>0.3</startWickHitPointsPercent>
				<extraInspectStringKey>ExplosionOnDamageWarning</extraInspectStringKey>
			</li>
		</comps>
	</ThingDef>
	<ThingDef Name="VFEPD_ColourableFurnitureBase" ParentName="BuildingBase" Abstract="True">
		<building>
			<paintable>true</paintable>
		</building>
		
	</ThingDef>
	<ThingDef Abstract="True" ParentName="BuildingBase" Name="DecalBase">
		<description>A small floor decal.</description>
		<thingClass>VFEProps.Building_SubstractsSilver</thingClass>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>Transparent</shaderType>
			<color>(255,255,255,65)</color>
		</graphicData>
		<rotatable>true</rotatable>
		<altitudeLayer>Floor</altitudeLayer>
		<passability>Standable</passability>
		<pathCost>0</pathCost>
		<drawerType>MapMeshOnly</drawerType>
		<building>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<paintable>true</paintable>
		</building>
		<castEdgeShadows>false</castEdgeShadows>
		<fillPercent>0</fillPercent>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>0.1</Mass>
			<Flammability>0</Flammability>
			<Beauty>1</Beauty>
		</statBases>
		<size>(1,1)</size>
		<uiIconScale>1</uiIconScale>
		<filthLeaving Inherit="False"/>
		<clearBuildingArea>false</clearBuildingArea>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<comps>
			<li Class="VFEProps.CompProperties_SubstractSilver">
				
			</li>
		</comps>
	</ThingDef>
	<ThingDef Abstract="True" ParentName="DecalBase" Name="DecalBaseLarge">
		<description>A large floor decal.</description>
		<size>(1,1)</size>
	</ThingDef>
</Defs>
