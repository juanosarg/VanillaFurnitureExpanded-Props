<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_BangBus</defName>
		<label>Bang Bus</label>
		<description>A basic passenger transport, the Bangbus is a relatively cheap piece of machinery to construct to transport people across roads of flat terrain. Non-functional: all its internal wiring and its motor have been removed.</description>
		<graphicData>
			<texPath>Things/Vehicles/Tier1/Tier1_Van/Tier1_Van</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,5)</drawSize>
			<color>(160, 160, 160)</color>
			<shaderType>CutoutComplex</shaderType>
			<shadowData>
				<volume>(1.8, 0.7, 3.8)</volume>
			</shadowData>
		</graphicData>
		
		<altitudeLayer>Building</altitudeLayer>
		<defaultPlacingRot>South</defaultPlacingRot>
		<blockWind>true</blockWind>
		<passability>Impassable</passability>
		<fillPercent>0.5</fillPercent>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<repairEffect>ConstructMetal</repairEffect>
		<statBases>
			<MaxHitPoints>450</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>200</Mass>
			<Flammability>0.2</Flammability>
		</statBases>
		<size>(2,4)</size>
		<uiIconScale>1</uiIconScale>
	</ThingDef>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_Bunsen</defName>
		<label>Bunsen</label>
		<description>A basic light tank, the Bunsen is a modified armoured vehicle suited for taking on enemy fortifications and exposed infantry. Non-functional: all its internal wiring and its motor have been removed.</description>
		<thingClass>VFEProps.Building_VehicleWithTurret</thingClass>
		<drawerType>RealtimeOnly</drawerType>
		<tickerType>Normal</tickerType>
		<graphicData>
			<texPath>Things/Vehicles/Tier1/Tier1_FlamerTank/Tier1_FlamerTank</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,5)</drawSize>
			<color>(72, 77, 72)</color>
			<shaderType>CutoutComplex</shaderType>
			<shadowData>
				<volume>(1.8, 0.7, 3.8)</volume>
			</shadowData>
		</graphicData>
		
		<altitudeLayer>Building</altitudeLayer>
		<defaultPlacingRot>South</defaultPlacingRot>
		<blockWind>true</blockWind>
		<passability>Impassable</passability>
		<fillPercent>0.5</fillPercent>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<repairEffect>ConstructMetal</repairEffect>
		<statBases>
			<MaxHitPoints>450</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>200</Mass>
			<Flammability>0.2</Flammability>
		</statBases>
		<size>(2,4)</size>
		<uiIconScale>1</uiIconScale>
		<modExtensions>
            <li Class="VFEProps.DrawTurretExtension">
                <turretToDraw>Things/Vehicles/Tier1/Tier1_FlamerTank/Tier1_FlamerTank_CannonTop</turretToDraw>
				<drawSize>(3,3)</drawSize>
				<offset>(0, 0.4)</offset>
            </li>
        </modExtensions>
	</ThingDef>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_Dinghy</defName>
		<label>Dinghy</label>
