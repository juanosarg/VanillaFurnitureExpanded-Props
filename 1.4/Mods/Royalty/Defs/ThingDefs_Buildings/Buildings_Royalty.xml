<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_DrapeProp</defName>
		<label>decorative drape</label>
		<description>A decorative drape. This will not satisfy any noble requirements, as it is just for decorative purposes.</description>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<passability>Standable</passability>
		<fillPercent>0.40</fillPercent>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>15</Mass>
			<Flammability>1.0</Flammability>
		</statBases>
		<canOverlapZones>true</canOverlapZones>
		<pathCost>10</pathCost>
		<rotatable>true</rotatable>
		<size>(2,1)</size>
		<uiIconOffset>(0, 0.15)</uiIconOffset>
		<filthLeaving>Filth_Trash</filthLeaving>
		<graphicData>
			<drawSize>(4.2,4.2)</drawSize>
			<texPath>Things/Building/Furniture/Drape</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<shadowData>
				<volume>(2,0.5,0.3)</volume>
				<offset>(0,0,-0.2</offset>
			</shadowData>
			<damageData>
				<rectN>(0.1, 0.1, 1.8, 1.5)</rectN>
				<rectS>(0.1, 0.1, 1.8, 1.5)</rectS>
				<rectE>(0.25, 0.5, 0.5, 1.5)</rectE>
				<rectW>(0.25, 0.5, 0.5, 1.5)</rectW>
			</damageData>
		</graphicData>
	</ThingDef>
	<ThingDef ParentName="VFEPD_ColourableFurnitureBase">
		<defName>VFEPD_Shuttle</defName>
		<label>shuttle</label>
		<description>An out-of-service imperial shuttle.</description>
		<graphicData>
			<texPath>Things/Building/Misc/Shuttle</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			
			<drawSize>(5,3)</drawSize>
			<shadowData>
				<volume>(0.55, 0.30, 0.40)</volume>
			</shadowData>
			
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		
		<castEdgeShadows>False</castEdgeShadows>
		<blockWind>true</blockWind>
		<passability>Impassable</passability>
		<fillPercent>0.5</fillPercent>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<repairEffect>ConstructMetal</repairEffect>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>0</WorkToBuild>
			<Mass>200</Mass>
			<Flammability>0.2</Flammability>
		</statBases>
		<size>(5,3)</size>
		<uiIconScale>1</uiIconScale>
	</ThingDef>
	
</Defs>
